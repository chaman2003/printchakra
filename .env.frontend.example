# PrintChakra Frontend Environment Configuration
# Copy values and set in your frontend/.env or frontend/.env.local file

# Backend API URL Configuration
# Leave commented for local development (auto-detects http://localhost:5000)
# For production deployment, set to your backend URL
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_URL=https://your-backend-url.ngrok-free.dev

# Frontend Port 
PORT=3000

# Build Configuration
DISABLE_ESLINT_PLUGIN=true
GENERATE_SOURCEMAP=false            

